                                emailDiv = `<span style="color:blue;font-weight:bold;word-break:break-word">${details.email} (Snov.io)</span>`;
                            }
                            $(".hr_details_linkedin").append(`<li>
                                <div class="img">
                                    <img src="${details.image_url}" />
                                </div>
                                <div class="text">
                                    <a target="_blank" href="${details.profile_url}">${details.name}</a>
                                    ${emailDiv}
                                    <span style="color:green">${details.place}</span>
                                    <span>${details.job_title}</span>
                                </div>
                            </li>`);
                        }
                    });

                    console.log('linkedinDataArray modify', linkedinDataArray);
                });
        }

        function toggle(source) {
            var checkboxes = document.querySelectorAll('input[type="checkbox"]');
            let arrayOfEmail = [];
            for (var i = 0; i < checkboxes.length; i++) {
                if (checkboxes[i] != source)
                    checkboxes[i].checked = source.checked;

                    arrayOfEmail.push(checkboxes[i].value);
                    
            }
            
            console.log('cnm',arrayOfEmail);
            localStorage.setItem("selectedEmail",JSON.stringify(arrayOfEmail));
            
        }
        localStorage.setItem("selectedEmail",null);
        function tdClick(e, email) {
            if (!$("#checkbox-" + e).is(':checked')) {
                $("#checkbox-" + e).prop("checked", true);
            } else {
                $("#checkbox-" + e).prop("checked", false);
            }

            //store email
            // localStorage.setItem("selectedEmail",JSON.stringify(['vv','mm']))
            let arrayOfEmail = []
            let selectedEmail = localStorage.getItem("selectedEmail");
            
            if(selectedEmail !== 'null'){
                arrayOfEmail = JSON.parse(selectedEmail);
            }
            
            arrayOfEmail.push(email);
            localStorage.setItem("selectedEmail",JSON.stringify(arrayOfEmail));
            
            console.log(arrayOfEmail);

            

        };
        function send_invitation() {
            window.location.href = "/send-invitation";
        }
    </script>

